<!DOCTYPE html>
<html>
<head>
  <title>FitFight</title>
<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>


  </head>
<body>

  <% if session[:user_id] %>
    <% user = User.find(session[:user_id]) %>
  <% end %>
  <div id="top">
    <header>
    <!-- LOGO IMAGE LINK -->
    </header>



 <nav class="navbar_bar">
        <div class='logo_font'>
            <%= link_to "FITFIGHT", user_challenges_path(@user), :class => "logo_nav" %>

          <ul class="navbar">
            <% if session[:user_id] %>
              <% user = User.find(session[:user_id]) %>
              <li class="nav_selection"><%= link_to user.fullname, edit_user_path(session[:user_id]) %></li>
              <li class="nav_selection"><%= link_to 'Logout', session_path(user), method: :delete %></li>
            <% end %>
          </ul>
            </div>
 </nav>

   <div class="container">
    <br><br><br><br><br><br><br><br>
      <!-- /.container -->



<%= yield %>

  </div>
</body>
</html>
