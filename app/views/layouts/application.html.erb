<!DOCTYPE html>
<html>
<head>
  <title>FitFight</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav>
    <ul>
      <% if session[:user_id] %>
        <li><%= link_to 'Logout', session_path(User.find(session[:user_id])), method: :delete %></li>
      <% end %>
    </ul>
  </nav>
<%= yield %>

</body>
</html>
